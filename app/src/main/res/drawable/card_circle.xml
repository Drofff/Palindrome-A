<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
<item android:height="95dp" android:width="95dp">
    <shape
        android:shape="oval"
        android:useLevel="false" >
        <stroke
            android:width="16dp"
            android:color="@color/card_background" />
    </shape>
</item>
<item android:height="70dp" android:width="70dp" android:start="15dp" android:top="15dp">
    <shape
        android:shape="oval"
        android:useLevel="false" >
        <solid android:color="@android:color/transparent" />
        <stroke
            android:width="1dp"
            android:color="@color/card_background" />
    </shape>
</item>
</layer-list>